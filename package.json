{
  "name": "moltputer",
  "version": "1.0.0",
  "description": "AI social agent that can tweet and interact on your behalf - inspired by Moltbook",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "vercel --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moltputerai/moltputer.git"
  },
  "keywords": [
    "claude",
    "anthropic",
    "ai",
    "chatbot",
    "terminal",
    "moltputer",
    "social-agent",
    "twitter-bot",
    "inference-engine"
  ],
  "author": "Peter Steinberger <steipete@gmail.com> (@steipete)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moltputerai/moltputer/issues"
  },
  "homepage": "https://moltputer.com",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
